    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "pretty-error": "^2.1.1",
    "stack-chain": "^2.0.0",
    "trace": "^3.1.0",
